@import"../../lib/style/iconfont.wxss";
page{
  height: 100%;
  background: url(data:image/jpeg;base64,)
}
.topback{
  width: 750rpx;
  height: 128rpx;
}
.goBack{
  position: fixed;
  top:40rpx;
  left: 10rpx;
}
.mainView{
  display: flex;
  flex-direction: column;
  width: 710rpx;
  height: 1170rpx;
  margin-left: 20rpx;
  background: white;
  border-radius: 8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  align-items: center; 
}
.top{
  width: 360rpx;
  height: 204rpx;
  margin-top: 30rpx;
  box-shadow: 0px 0px 15rpx 0px rgba(234, 234, 234, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}
.tmName{
  font-size: 51rpx;
}
.name{
  width: 710rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  margin-top: 20rpx;
}
.tmDetail{
  width: 710rpx;
  height: 120rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin-top: 40rpx;
}
.detail-item{
  width: 220rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.change{
  font-size: 34rpx;
  color: #888888;
}
.static{
  font-size: 22rpx;
  margin-top: 10rpx
}
.top-tab{
  width: 710rpx;
  height: 68rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  margin-top: 40rpx
}
.top-tab-item{
  height: 58rpx;
  font-size: 34rpx;
  color: #888888;
}
.top-tab-item.active{
  border-bottom: 2px solid #e50011;
  font-size: 34rpx;
  color: black;
}
.grad{
  height: 68rpx;
  width: 2px;
  background: radial-gradient(#ff7a7a 30%, white 70%); /* 标准的语法（必须放在最后） */
}
.swiper{
  width:100%;
  display: flex;
  flex-direction: column;
}
.content{
  box-sizing: border-box;
  width: 670rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
  margin-left: 20rpx;
  padding-bottom: 40rpx;
}
.lawDate-title{
  width: 630rpx;
  font-size: 34rpx;
  margin:40rpx auto auto 20rpx;
}
.lawDate{
  width: 630rpx;
  display: flex;
  flex-direction: column;
  border-radius: 8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  box-shadow: 0px 0px 15rpx 0px rgba(234, 234, 234, 1);
  margin:20rpx auto 4rpx 20rpx;
  padding-bottom: 40rpx;
}
.moreDate{
  display: flex;
  flex-direction: row;
  align-items: center;
  width:100%;
  margin-top: 40rpx;
}
.dateBefore{
  font-size: 28rpx;
  margin-left: 30rpx;
}
.dateAfter{
  font-size: 28rpx;
  margin:auto 30rpx auto auto;
  color: #888888; 
}
.lawRole{
  width: 630rpx;
  display: flex;
  flex-direction: column;
  margin-top: 30rpx;
  margin-left: 18rpx
}
.applicant{
  width:570rpx;
  font-size: 26rpx;
}
.applicant-real{
  width:570rpx;
  font-size: 26rpx;
  color:#888888;  
	word-break:break-all;
}
.progress{
  box-sizing: border-box;
  width: 630rpx;
  display: flex;
  flex-direction: column;
  border-radius: 8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  box-shadow: 0px 0px 15rpx 0px rgba(234, 234, 234, 1);
  margin:20rpx auto 4rpx 20rpx;
  padding: 35rpx 30rpx 0 30rpx;
}
.progress-item{
  display: flex;
  flex-direction: column;
  border-bottom: 1rpx solid #e5e5e5;
}
.progress-detail{
  height: 90rpx;
  display: flex;
  flex-direction: row;
  align-items: center; 
}
.progress-detail-spread{
  display: flex;
  flex-direction: column;
}
.progress-box{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 40rpx;
  height: 40rpx;
  border: 4rpx solid #eaeaea;
  border-radius: 10rpx;
}
.progress-name{
  font-size: 34rpx;
  margin-left: 20rpx;
}
.check-information{
  font-size: 26rpx;
  color:#888888;
  margin:auto 20rpx auto auto;
}
.judge{
  box-sizing: border-box;
  width: 630rpx;
  display: flex;
  flex-direction: column;
  border-radius: 8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  box-shadow: 0px 0px 15rpx 0px rgba(234, 234, 234, 1);
  margin:20rpx auto 4rpx 20rpx;
  padding: 40rpx 0 40rpx 0;
}
.judge-top{
  display: flex;
  flex-direction: row;
  width: 630rpx;
  align-items: center;
}
.similar-judge{
  background-color: #ff4242;
  font-size: 22rpx;
  color: white;
  margin-left: 30rpx;
}
.similar-name{
  font-size: 26rpx;
  margin-left: 6rpx;
}
.tm-state{
  font-size: 22rpx;
  color: #888888;
  margin:auto 0 auto auto;
}
.judge-cls{
  font-size: 22rpx;
  color: #999999;
  margin-left: 122rpx;
}
.judge-regNo{
  font-size: 22rpx;
  color: #999999;
  margin-left: 60rpx;
}
.judge-applicant{
  font-size: 22rpx;
  color: #999999;
  margin-left: 122rpx;
}
.judge-bottom{
  display: flex;
  flex-direction: row;
  width: 630rpx;
  margin-top: 10rpx;
}
.judge-advice{
  height: 38rpx;
  line-height: 38rpx;
  font-size: 22rpx;
  color: #888888;
  margin-left: 30rpx;
  justify-content:center;
}
.judge-advice-methods{
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  width:500rpx;
}
.advice-method{
  height: 38rpx;
  font-size: 22rpx;
  text-align: center;
  line-height: 38rpx;
  background: #eaeaea;
  border-radius: 20rpx;
  padding: 0 30rpx 0 30rpx;
  margin-right: 25rpx;
  margin-bottom: 14rpx; 
}
.add-bottom{
  width:64rpx;
  height:64rpx;
  border-radius: 50%;
  -moz-border-radius: 50%;
  -webkit-border-radius: 50%;
  position: fixed;
  bottom: 55rpx;
  right: 40rpx;
}
.img-bottom{
  width:64rpx;
  height:64rpx;
}


/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.tmImg{
	width: 100%;
	height: 100%;
}
.flow-table{
	width: 668rpx;
	box-shadow: 0px 0px 15rpx 0px rgba(234, 234, 234, 1);
	border-radius: 10rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	margin-top: 40rpx;
	margin-bottom: 20rpx;
}
.flow-row-first{
	display: flex;
	align-items: center;
	justify-content: center;
}
.flow-row-title{
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 26rpx;
	color: black;
	height: 110rpx;
}
.flow-row-title:nth-child(1){
	width: 216rpx;
}
.flow-row-title:nth-child(2){
	width: 240rpx;
}
.flow-row-title:nth-child(3){
	width: 208rpx;
}
.flow-row{
	width: 668rpx;
	box-shadow: 0px 0px 15rpx 0px rgba(234, 234, 234, 1);
	border-radius: 10rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}
.flow-row-content{
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 24rpx;
	color: black;
	height: 110rpx;
}
.flow-row-content:nth-child(1){
	width: 216rpx;
}
.flow-row-content:nth-child(2){
	width: 240rpx;
}
.flow-row-content:nth-child(3){
	width: 208rpx;
}