@import"../../lib/style/iconfont.wxss";
page{
  background-color: #eaeaea;
}
.title-bar{
  display: flex;
  width:750rpx;
  height: 230rpx;
  background: white;
  position: fixed;
  top:0;
  z-index: 100;
}
.bar-item{
  display: flex;
  width:375rpx;
  height: 100rpx;
  align-items: center;
  justify-content: center;
  margin-top: 130rpx
}
.bar-text{
  font-size: 34rpx
}
.bar-item.active{
  border-bottom: 2px solid #e50011;
  color: #e50011;
}
.swiper{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
  width: 100%;
  margin-top: 230rpx;
}
.content-item{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
}
.content{
  width: 718rpx;
  height: 174rpx;
  border-radius:8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  background: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top:16rpx;
  margin-left: 16rpx;
}
.content-img{
  width:150rpx;
  height: 120rpx;
  border-radius:8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
}
.tmImg{
  width:100%;
  height: 100%;
  border-radius:8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
}
.tmDetail{
  width: 360rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}
.tmName{
  width: 360rpx;
  height: 35rpx;
  font-size: 28rpx;
  margin-left: -6rpx;
}
.register{
  width: 360rpx;
  height: 35rpx;
  font-size: 26rpx;
}
.regNo{
  width: 360rpx;
  height: 30rpx;
  font-size: 22rpx;
  color: #888888;
  margin: auto auto 0 auto;
}
.date{
  width: 150rpx;
  height: 120rpx;
  font-size: 22rpx;
  color: #888888;
}
.progress{
  width: 718rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
  border-radius:8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  background: white;
  margin-top: 16rpx;
  margin-left: 16rpx;
}
.month{
  width: 670rpx;
  height: 72rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.month-title{
  font-size: 34rpx;
  margin-left: 10rpx;
 }
 .details{
   display: flex;
   flex-direction: column;
   width: 670rpx;
   border-radius:8rpx;
  -moz-border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  box-shadow: 0px 0px 15rpx 0px rgba(234, 234, 234, 1);
  margin-bottom: 16rpx;
  align-items: center;
  justify-content: center;
 }
 .detail{
   width: 642rpx;
   height: 180rpx;
   display: flex;
   flex-direction: row;
   align-items: center;
   justify-content: center;
 }
 .detail-mid{
  width: 440rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
 }
 .accept{
   width: 440rpx;
   font-size: 26rpx;
   color: #353535;
 }
 .intCls{
  width: 440rpx;
  height: 30rpx;
  font-size: 22rpx;
  color: #888888;
  margin: auto auto 0 auto;
 }
 .result{
   width: 642rpx;
   border-style: solid none none none;
   border-color: #e5e5e5;
   border-width:1rpx;
   font-size: 26rpx;
   color: #353535;
   padding: 20rpx 0 20rpx 0;
 }
 .add-bottom{
  width:64rpx;
  height:64rpx;
  border-radius: 50%;
  -moz-border-radius: 50%;
  -webkit-border-radius: 50%;
  position: fixed;
  bottom: 55rpx;
  right: 40rpx;
}
.img-bottom{
  width:64rpx;
  height:64rpx;
}
 /*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}